@model List<Contact>
@{
    ViewData["Title"] = "ContactList";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>İletişim</h2>

<table class="table table-bordered">

    <tr>

        <th>Adres</th>
        <th>Telefon</th>
        <th>Email</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
   
            <td>@item.Address</td>
            <td>@item.PhoneNumber</td>
            <td>@item.Email</td>
            <td><a href="/Contact/UpdateContact/@item.ContactId" class="btn btn-success">Güncelle</a></td>
        </tr>
    }
</table>


