@model List<Feature>
<!-- Feature Section -->
<section id="hero" class="hero section light-background">

    <img src="/images/about-me.jpeg" alt="">

    <div class="container" data-aos="zoom-out">
        <div class="row justify-content-center">
            <div class="col-lg-9">
                <h2>Gizem Hazal Akbulut</h2>
                <p>
                    Çalışma Alanlarım: <span class="typed" data-typed-items="@string.Join(", ", Model.Select(x => x.Title))">
                        @Model.FirstOrDefault()?.Title
                    </span><span class="typed-cursor typed-cursor--blink" aria-hidden="true"></span>
                </p>

				@await Component.InvokeAsync("_DefaultSocialMediaComponentPartial")
            
            </div>
        </div>
    </div>

</section>
<!-- /Feature Section -->
